  "description": "Encrypt and decrypt in-memory at runtime your .env file so you can store sensitive information (passwords etc.) in source control",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "mocha --reporter spec --timeout 100000 --exit",
    "build": "tsc"
  },
  "repository": {
