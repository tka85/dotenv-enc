    "ts-node": "10.9.1",
    "typescript": "5.0.2"
  },
  "devDependencies": {
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "chai": "4.3.7",
    "eslint": "8.36.0",
    "eslint-plugin-import": "2.27.5",
    "mocha": "10.2.0"
  }
